{
    "name": "elevatecommerce/visual-editor",
    "description": "Visual Editor package with admin authentication and extensible navigation registry",
    "type": "library",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "ElevateCommerce\\VisualEditor\\": "src/",
            "ElevateCommerce\\VisualEditor\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ElevateCommerce\\VisualEditor\\VisualEditorServiceProvider"
            ],
            "aliases": {
                "Navigation": "ElevateCommerce\\VisualEditor\\Facades\\Navigation",
                "Dashboard": "ElevateCommerce\\VisualEditor\\Facades\\Dashboard",
                "Context": "ElevateCommerce\\VisualEditor\\Facades\\Context",
                "Section": "ElevateCommerce\\VisualEditor\\Facades\\Section"
            }
        }
    }
}
